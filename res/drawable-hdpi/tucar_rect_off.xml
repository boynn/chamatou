<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- 连框颜色值 -->
    <item>
        <shape>
            <solid android:color="#FFDDDDDD" />
        </shape>
    </item>
    <!-- 主体背景颜色值 -->
    <item
        android:bottom="1dip"
        android:left="1dip"
        android:right="1dip"
        android:top="1dip">
        <shape>
            <solid android:color="#FFFFFFFF" />
        </shape>
    </item>

</layer-list>